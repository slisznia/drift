import std.console.out

fn may_fail_error(flag: Int) -> Int {
  if flag == 0 {
    return 0
  } else {
    throw drift_error_new_dummy(flag, "payload", "")
  }
}

fn main() -> Int {
  try {
    may_fail_error(1)
  } catch err {
    out.writeln("err")
    return 1
  }
  out.writeln("ok")
  return 42
}
