import std.console.out

exception InvalidPayload(payload: String)

fn ingest(line: String) -> Void {
  val ^raw: String as "ingest.payload" = line

  if line == "" {
    throw InvalidPayload(payload = line)
  } else {
    out.writeln("accepted: " + line)
  }
}

fn main() -> Int {
  ingest("alpha")
  ingest("")
  return 0
}
