exception MyErr(msg: String)

fn foo(flag: Int) -> Int {
  if flag == 1 {
    throw MyErr(msg = "one")
  }
  throw drift_error_new_dummy(flag, "payload", "")
}

fn main() -> Int {
  val x: Int = try foo(1) catch MyErr(e) { 10; } catch err { 20; }
  val y: Int = try foo(2) catch MyErr(e) { 10; } catch err { 20; }
  return x + y
}
