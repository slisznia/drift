exception MyErr(msg: String)

fn foo(flag: Int) returns Int {
  if flag == 1 {
    throw MyErr(msg = "one")
  }
  throw drift_error_new_dummy(flag, "")
}

fn main() returns Int {
  try {
    foo(1)
  } catch MyErr(e) {
    out.writeln("event")
  } catch err1 {
    out.writeln("fallback")
  }
  try {
    foo(2)
  } catch MyErr(e) {
    out.writeln("event")
  } catch err2 {
    out.writeln("fallback")
  }
  return 0
}
