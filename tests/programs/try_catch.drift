import sys.console.out

exception InvalidDate(input: String)

fn parse_date(input: String) returns String {
	if input == "" {
		throw InvalidDate(input = input)
	}
	return input
}

fn main() returns Void {
	try {
		out.writeln(parse_date("2025-11-20"))
		out.writeln(parse_date(""))
	} catch InvalidDate(e) {
		out.writeln("caught InvalidDate")
	}
	val fallback = try parse_date("") else "1970-01-01"
	out.writeln(fallback)
}
