exception MyError(a: String, b: String)

fn make_error() -> Void {
  throw MyError(a = "AA", b = "BBB")
}

fn main() -> Int {
  try {
    make_error()
  } catch MyError(e) {
    val _a = e.attrs["a"].as_string().unwrap_or("<missing>")
    val _b = e.attrs["b"].as_string().unwrap_or("<missing>")
    return 0
  }
  return 0
}
