import std.console.out

fn foo(x: Int64) returns Int64 {
  return x
}

fn might_fail(x: Int64) returns Int64 {
  return x
}

fn main() returns Int64 {
  val y: Int64 = try foo(1) else 42
  out.writeln("computed y")

  try {
    out.writeln("before")
    might_fail(1)
  } catch err {
    out.writeln("caught")
  }

  val rc: Int64 = 0
  return rc
}
