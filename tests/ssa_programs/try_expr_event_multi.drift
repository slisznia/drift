exception Foo(msg: String)
exception Bar(msg: String)

fn boom(x: Int) -> Int {
  if x == 1 {
    throw Foo(msg = "foo")
  }
  if x == 2 {
    throw Bar(msg = "bar")
  }
  return x
}

fn main() -> Int {
  val a: Int = try boom(1) catch Foo(e) { 10; } catch { -1; }
  val b: Int = try boom(2) catch Foo(e) { 10; } catch { -2; }
  return a + b
}
