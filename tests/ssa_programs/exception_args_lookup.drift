exception MyError(a: String, b: String)

fn make_error() returns Void {
  throw MyError(a = "A", b = "B")
}

fn main() returns Int {
  var sum: Int = 0
  try {
    make_error()
  } catch MyError(e) {
    val view = e.args
    // Required access via method sugar.
    val a_val: String = view[view.a()]
    val b_val: String = view[.b]
    // Optional path via explicit ArgKey construction.
    val manual_key = MyErrorArgKey(name = "a")
    val a_opt = view[manual_key]
    sum = 0
  }
  return sum
}
