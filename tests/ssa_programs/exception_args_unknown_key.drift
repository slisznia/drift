exception MyError(a: String)

fn raise_it() -> Void {
  throw MyError(a = "AA")
}

fn main() -> Int {
  try {
    raise_it()
  } catch MyError(e) {
    val missing = e.attrs["c"].as_string()
    // Missing key yields none; unwrap to default to prove path works.
    val fallback = missing.unwrap_or("none")
    return 0
  }
  return 0
}
