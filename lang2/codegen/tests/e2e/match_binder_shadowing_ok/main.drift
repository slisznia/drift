module main

variant Maybe<T> {
	Some(value: T),
	None,
}

fn main() nothrow -> Int{
	val x: Maybe<Int> = Maybe::Some(1);
	val y = match x{
	Some(v) => {
			// Shadow the binder name inside the arm block. This must not corrupt
			// the binder binding or generate invalid SSA/PHI nodes.
			val v = v + 1;
			v
		},
		None => { 0 },
	};
	return y;
}
