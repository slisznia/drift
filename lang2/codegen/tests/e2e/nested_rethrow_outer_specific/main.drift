module main

exception A()
exception B()

fn run() returns Int {
    try {
        try {
            throw A();
        } catch A(a) {
            print("inner");
            throw B();
        }
        print("UNREACHABLE");
    } catch B(b) {
        print("outer");
    }
    return 0;
}

fn main() returns Int  nothrow{
    try run() catch { return 0; }
    return 0;
}
