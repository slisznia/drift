module main

variant Maybe<T> {
	Some(value: T),
	None,
}

fn foo() nothrow -> Maybe<Int> {
	// Constructor calls require an expected type; return type provides it.
	return Some(41);
}

fn main() nothrow -> Int{
	val x: Maybe<Int> = foo();
	val y = match x{
	Some(v) => { v + 1 },
		None => { 0 },
	};
	return y;
}
