module m

struct Point(x: Int)

implement Point {
	fn bump_and_sum(self: &mut Point) nothrow -> Int {
		// Local bindings shadow implicit `self` members.
		val x: Int = 1;

		// Explicit member-through-reference access remains available when the
		// local name shadows a field.
		self->x += 1;

		return x + self->x;
	}
}

fn main() nothrow -> Int{
	var p: Point = Point(1);
	return p.bump_and_sum();
}
