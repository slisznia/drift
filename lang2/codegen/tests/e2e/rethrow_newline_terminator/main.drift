module main

exception E()

fn run() returns Int {
    try {
        try {
            throw E();
        } catch err {
            print("inner");
            rethrow;
        }
        print("UNREACHABLE");
    } catch err {
        print("outer");
    }
    return 0;
}

fn main() nothrow returns Int{
    try run() catch { return 0; }
    return 0;
}
