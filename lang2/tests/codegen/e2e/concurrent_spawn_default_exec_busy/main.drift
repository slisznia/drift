module main

import std.concurrent as conc;
import std.core as core;
import lang.thread as thread;

fn main() nothrow -> Int {
	thread.exec_submit_test_override(1);
	var t = conc.spawn(| | => { return 7; });
	var res = t.join();
	return match res {
		Err(err) => {
			match err {
				Failed(_e) => { 0 },
				default => { 1 },
			}
		},
		Ok(_v) => { 2 },
		default => { 2 },
	};
}
