module main

import std.err;
import std.iter as iter;
import std.containers as containers;

fn main() nothrow -> Int {
var dq = containers.deque<type Int>();
	dq.push_back(1);
	var r = dq.range();
	dq.push_back(2);
	dq.push_back(3);
	val out = try iter.RandomAccessReadable::len(&r) catch std.err:IteratorInvalidated(_e) {
		0
	} catch {
		99
	};
	return out;
}