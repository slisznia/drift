module main

import std.containers as containers;

fn run() -> Int {
	var m = containers.hash_map<type Int, String>();
	m.insert(1, "a");
	val old = m.insert(1, "b");
	val old_v = match old {
		None => { "" },
		Some(v) => { v }
	};
	if old_v == "a" {
	} else {
		return 1;
	}
	val k = 1;
	val cur = match m.get_mut(&k) {
		None => { "" },
		Some(p) => { *p }
	};
	if cur == "b" {
	} else {
		return 2;
	}
	return 0;
}

fn main() nothrow -> Int {
	return try run() catch { 99 };
}
