module main

import std.algo as algo;
import std.containers;

fn main() nothrow -> Int {
var dq: std.containers.Deque<Int> = std.containers.deque_new<type Int>();
	dq.push_back(4);
	dq.push_back(5);
	dq.push_back(6);
	val _p1 = dq.pop_front();
	val _p2 = dq.pop_front();
	dq.push_back(3);
	dq.push_back(2);
	dq.push_back(1);
	var rm = dq.range_mut();
	algo.sort_in_place(&mut rm);
	var r = dq.range();
	val idx = algo.binary_search(&r, &5);
	val out: Int = match idx {
		Some(i) => {
			(*dq.at(i) == 5) ? 0 : 1
		},
		None => {
			2
		},
	};
	return out;
}
