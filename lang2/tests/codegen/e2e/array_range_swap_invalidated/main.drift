module main

import std.err;
import std.iter as iter;

fn main() -> Int {
	var arr = [1, 2];
	var r = arr.range_mut();
	r.__test_invalidate();
	val out = try {
		iter.RandomAccessPermutable::swap(&mut r, 0, 0);
		0
	} catch std.err:IteratorInvalidated(e) {
		val dv_c = e.attrs["container_id"];
		val dv_o = e.attrs["op_id"];
		val c = dv_c.as_string();
		val o = dv_o.as_int();
		val c_code = match c {
			Some(s) => {
				(s != "std.containers:Array") ? 10 : 0
			},
			None => {
				11
			},
		};
		val o_code = match o {
			Some(v) => {
				(v != 4) ? 12 : 0
			},
			None => {
				13
			},
		};
		(c_code != 0) ? c_code : ((o_code != 0) ? o_code : 0)
	} catch {
		99
	};
	return out;
}
