module m

exception MyErr(msg: String)
exception Other()

fn foo(flag: Int) -> Int {
	if flag == 1 {
		throw MyErr(msg = "one");
	}
	throw Other();
}

fn main() nothrow -> Int{
val x = try foo(1) catch MyErr(e) { 10 } catch { 20 };
val y = try foo(2) catch MyErr(e) { 10 } catch { 20 };
	return x + y;
}
